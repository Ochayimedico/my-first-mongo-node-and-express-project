<%- include('../partials/head')%> 

    <title>Products</title>
  </head>
  <body >
    <section class=" container w-70 p-3 ">
    <h1>All Products</h1>
    <ul class="list-unstyled">
      <% for (let product of products){ %>
      <div class="list mb-2">
          <a class="list-group-item list-group-item-action links" aria-current="true" href="/products/<%=product._id%>"><%= product.name %></a>
      </div>
      <%}%>
    </ul>
    <a class="btn btn-primary" href="/products/new">Add New Product</a>
  </body></section>
</html>
